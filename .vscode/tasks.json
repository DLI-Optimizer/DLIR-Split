{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ 生成活动文件",
            "command": "/usr/bin/g++",
            "args": [
                "-std=c++17",
                "-fdiagnostics-color=always",
                "-Wl,-rpath,${workspaceFolder}/third_party/lib",
                "-o",
                "${workspaceRoot}/bin/release/${fileBasenameNoExtension}",
                "${file}",
                "-I${workspaceFolder}/third_party/include",
                "-I${workspaceFolder}/",
                "-I${workspaceFolder}/include",
                "-I${workspaceFolder}/library",
                "-L${workspaceFolder}/third_party/lib",
                "${workspaceFolder}/sources/*/*.cpp",
                "-DONNX_NAMESPACE=onnx",
                "-DONNX_ML=1",
                "-lonnx_shared",
                "-lpthread",
                "-lprotobuf",
                "-lstdc++fs",
                "-lonnxruntime"
                // "-g",
                // "${file}",
                // "${workspaceFolder}/include/*/*",
                // "${workspaceFolder}/sources/*/*",
                // "${workspaceFolder}/library/*/",
                // "-o",
                // "${workspaceRoot}/bin/debug/${fileBasenameNoExtension}",
                // "-I/usr/include/python3.8",
                // "-lstdc++fs",
                // "-lonnxruntime",
                // "-lprotobuf",
                // "-lpthread",
                // "-L${workspaceFolder}/library/"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: g++-8 build",
            "command": "/usr/bin/g++-8",
            "args": [
                "-std=c++17",
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${workspaceFolder}/include/*/*",
                "${workspaceFolder}/sources/*/*",
                "${workspaceFolder}/library/*/",
                "-o",
                "${workspaceRoot}/bin/debug/${fileBasenameNoExtension}",
                "-I/usr/include/python3.8",
                "-lpython3.8",
                "-lstdc++fs",
                "-lonnxruntime",
                "-lprotobuf",
                "-lpthread",
                "-L${workspaceFolder}/library/"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "调试器生成的任务。"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: g++-8 build-release",
            "command": "/usr/bin/g++-8",
            "args": [
                "-std=c++17",
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${workspaceFolder}/include/*/*",
                "${workspaceFolder}/sources/*/*",
                "-o",
                "${workspaceRoot}/bin/debug/${fileBasenameNoExtension}",
                "-I/usr/include/python3.8",
                "-I ${workspaceRoot}/include",
                "-lpython3.8",
                "-lstdc++fs",
                "-lonnxruntime",
                "-lprotobuf",
                "-lpthread",
                "-L${workspaceFolder}/library/"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "运行生成的任务。"
        }
    ],
    "version": "2.0.0"
}