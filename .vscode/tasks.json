{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ 生成活动文件",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: g++-8 build",
            "command": "/usr/bin/g++-8",
            "args": [
                "-std=c++17",
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${workspaceFolder}/include/*/*",
                "${workspaceFolder}/sources/*/*",
                "${workspaceFolder}/library/*/",
                "-o",
                "${workspaceRoot}/bin/debug/${fileBasenameNoExtension}",
                "-I/usr/include/python3.8",
                "-lpython3.8",
                "-lstdc++fs",
                "-lonnxruntime",
                "-lprotobuf",
                "-lpthread",
                "-L${workspaceFolder}/library/"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "调试器生成的任务。"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: g++-8 build-release",
            "command": "/usr/bin/g++-8",
            "args": [
                "-std=c++17",
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${workspaceFolder}/include/*/*",
                "${workspaceFolder}/sources/*/*",
                "-o",
                "${workspaceRoot}/bin/debug/${fileBasenameNoExtension}",
                "-I/usr/include/python3.8",
                "-I ${workspaceRoot}/include",
                "-lpython3.8",
                "-lstdc++fs",
                "-lonnxruntime",
                "-lprotobuf",
                "-lpthread",
                "-L${workspaceFolder}/library/"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "运行生成的任务。"
        }
    ],
    "version": "2.0.0"
}